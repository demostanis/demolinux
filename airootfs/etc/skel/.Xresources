URxvt.font: xft:FiraCode Nerd Font Mono:size=12,xft:Noto Sans Symbols 2:size=8,xft:Noto Color Emoji:size=9
URxvt.internalBorder: 5
URxvt.iconFile: /usr/share/icons/la-capitaine/apps/scalable/terminal.svg
URxvt.scrollBar: true
URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.C-minus: resize-font:smaller
URxvt.keysym.C-plus: resize-font:bigger
URxvt.keysym.C-equal: resize-font:reset
URxvt.perl-ext-common: default,-confirm-paste,-selection,vtwheel,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.rend.0: Uline
URxvt.matcher.button: 1
URxvt.iso14755: false

